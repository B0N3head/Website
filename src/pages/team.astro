          <!-- President -->
          <div style="max-width: 375px;">
            <img src="/src/styles/guy_icon.png" alt="Zac Knight" style="width: 100%; border-radius: 12px;" />
            <h2 style="margin-top: 10px;">Zac Knight</h2>
            <p style="color: #FF00E6;">President</p>
          </div>
          <!-- Vice President -->
