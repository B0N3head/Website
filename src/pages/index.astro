---
import "@/styles/global.css"
import KeyLogo from "@/logos/Logo-Full.svg"
import Button from "@/components/Button.astro"
import Navigation from "@/components/Navigation.astro"
import Footer from '../components/Footer.astro';
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<title>QUT AI & ML Society</title>
	</head>
	<body class="dark background">
		<Navigation />
		<main class="min-h-screen flex flex-col justify-center items-center p-12">

			<KeyLogo class="md:w-1/2"/>
			<h1 class="text-2xl text-center md:text-4xl font-heading p-8" style="color:white">Empowering the Future Through AI & Machine Learning</h1>
		</main>
		<Footer />
	</body>
</html>
